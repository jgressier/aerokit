[run]
  source = aerokit
  omit=
    */__init__.py
    aerokit/engine/*.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NameError
    raise NotImplementedError
    if 0:
    if __name__ == __main__:
    import doctest
    doctest.testmod()
